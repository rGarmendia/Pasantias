<?xml version="1.0" encoding="utf-8"?>
<?variable-resolver class="org.zkoss.zkplus.spring.DelegatingVariableResolver"?>
<?init class="org.zkoss.zkplus.databind.AnnotateDataBinderInit"  arg0="./editSolicitud" ?>

<zk xmlns:n="http://www.zkoss.org/2005/zk/native"
	xmlns:h="http://www.w3.org/1999/xhtml">


	<zscript src="/WEB-INF/commons/configureContext.zs" />
	<zscript src="/WEB-INF/commons/configureDialog.zs" />

	<zscript>
		<![CDATA[
		     
			org.modelo.dto.Solicitud solicitud = arg.get("objeto");
			List listaTipos = servicioAdministrarConfiguracion.buscarTiposDocumentosTodos();
			String modo = arg.get("modo");
			List listaUsuariosPermiso; 
			List aprobadores = new ArrayList(solicitud.aprobadores);
						
			if (solicitud.getUsuarioElaborador() == null)
			{
				solicitud.setUsuarioElaborador(usuario);
				solicitud.setUsuarioActual(usuario);
				solicitud.setCargoElaborador(((org.modelo.dto.Usuario) usuario).getCargo() == null ? "" : ((org.modelo.dto.Usuario) usuario).getCargo().getTitulo());
			}
			
			boolean desactivo = false;
			
			if (solicitud.getEstatus().equals("Nueva") || solicitud.getEstatus().equals("Rechazada"))
			{
				if (solicitud.getEstatus().equals("Rechazada"))
				{
					List temp = new ArrayList(solicitud.getAprobadores());
					
					for(org.modelo.dto.SolicitudAprobador sa : solicitud.getAprobadores())
					{
						org.modelo.dto.HistorialSolicitudAprobador hsa = new org.modelo.dto.HistorialSolicitudAprobador();
						hsa.setAprobador(sa.getAprobador());
						hsa.setAprobadorid(sa.getAprobadorid());
						hsa.setComentario(sa.getComentario());
						hsa.setDiasAprobacion(sa.getDiasAprobacion());
						hsa.setSolicitud(sa.getSolicitud());
						hsa.setSolicitudid(sa.getSolicitudid());
						hsa.setFechaAprobacion(sa.getFechaAprobacion());
						hsa.setFechaRecepcion(sa.getFechaRecepcion());
						hsa.setOrden(sa.getOrden());
						hsa.setResultado(sa.getResultado());
						
						solicitud.getHistoricoAprobadores().add(hsa);
					}
					solicitud.getAprobadores().clear();
					
					for (org.modelo.dto.SolicitudAprobador sa : temp)
					{
						sa.setComentario(null);
						sa.setFechaAprobacion(null);
						sa.setFechaRecepcion(null);
						sa.setResultado(null);
						solicitud.getAprobadores().add((org.modelo.dto.SolicitudAprobador)sa.clone());	
					}
				}
				else if (solicitud.getIdCodigo() == null)
				{
					org.modelo.dto.HistorialSolicitud hsol = new org.modelo.dto.HistorialSolicitud();
					hsol.setSolicitud(solicitud);
					hsol.setFecha(java.util.Calendar.getInstance().getTime());
					hsol.setOcurrencia("Se Creo el Documento");
					hsol.setUsuario(usuario);
					solicitud.getOcurrencias().add(hsol);
				}
				
				if (solicitud.getAprobadores().size() == 0)
				{
					org.modelo.dto.RutaAprobacionSeguridad ras = servicioAdministrarConfiguracion.buscarAprobadoresPorTipoArea(solicitud.getTipoDocumento(), solicitud.getArea(), solicitud.getArea());
					if (ras != null)
					{
						for(org.modelo.dto.RutaAprobacionAprobadorSolicitud raas : ras.getAprobadoresSolicitud())
						{
							org.modelo.dto.SolicitudAprobador sa = new org.modelo.dto.SolicitudAprobador();
							sa.setAprobador(raas.getAprobador());
							sa.setAprobadorid(raas.getAprobadorid());
							sa.setSolicitud(solicitud);
							sa.setOrden(raas.getOrden());
							sa.setDiasAprobacion(raas.getDiasAprobacion());
							solicitud.getAprobadores().add(sa);
						}
					}
					
				}
				desactivo = false;
			}
			else
			{
				desactivo = true;
			}
			
			void crear(Label lbl, Object hdoc)
			{
				try
				{
					if (hdoc instanceof org.modelo.dto.HistorialDocumento)
					{
						String newstring = new java.text.SimpleDateFormat("dd/MM/yyyy hh:MM:ss").format(hdoc.getFecha());
						lbl.setValue(newstring + " / " + hdoc.getUsuario().getNombres() + " "  + hdoc.getUsuario().getApellidos());
					}
					else if (hdoc instanceof org.modelo.dto.HistorialSolicitudAprobador)
					{
						if (hdoc.getFechaAprobacion() != null)
						{
							String newstring = new java.text.SimpleDateFormat("dd/MM/yyyy hh:MM:ss").format(hdoc.getFechaAprobacion());
							lbl.setValue(newstring + " / " + hdoc.getAprobador() .getNombres() + " "  + hdoc.getAprobador().getApellidos());
						}
					}
				}
				catch(Exception ex)
				{
					alert(ex.getMessage());
				}
			}
		]]>						
	</zscript>
	<window id="editSolicitud" title="${tipoDocumento.solicitud} (${solicitud.idCodigo})" maximizable="false" maximized="false" border="normal" width="95%" height="95%">
		<caption image="/images/window.png" />
		<style src="/css/mystyle.css" dynamic="true" />
		<div height="90%" style="overflow:auto">
			<textbox visible="false" />
			<borderlayout>
				<west title="Razon de Rechazo" size="17%" splittable="true" collapsible="true" if="${objetoP.estatus eq 'Rechazada'}">
					<div style="background:#B8D335">
						<textbox value="${comenRechazo}"
							style="color:white;font-size:25px" disabled="true" cols="10" rows="10"/>
					</div>
				</west>
				<center border="none" >
					<tabbox orient="horizontal">
						<tabs width="120px">
							<tab label="${labels.informacion}" />
							<tab label="${labels.aprobadores}" />
							<tab label="Historial de Ocurrencias" />
							<tab label="Historial de Aprobacion" />
						</tabs>
						<tabpanels>
							<tabpanel>
								<grid>
									<columns>
										<column width="20%" align="right" />
										<column width="80%" />
									</columns>
									<rows>
										<row if="${solicitud.estatus eq 'E' or solicitud.estatus eq 'C'}"> 
											<label class="obligatorio" value="${labels.codigo}" />
											<label value="@{solicitud.idCodigo}" />
										</row>
										<row>
											<label class="obligatorio" value="${labels.tipoDeDocumento}" />
											<radiogroup id="rgpTipo">
												<radio forEach="${listaTipos}" label="${each.nombre}" value="${each}" radiogroup="rgpTipo"></radio>
											</radiogroup>
										</row>
										<row>
											<label class="obligatorio" value="${labels.tipoDeCambio}" />
											<radiogroup id="rgpTipoS">
												<radio label="${labels.elaboracion}" value="Elaboracion"></radio>
												<radio label="${labels.modificacion}" value="Modificacion"></radio>
												<radio label="${labels.eliminacion}" value="Eliminacion"></radio>
												<radio label="${labels.impresion}" value="Impresion"></radio>
											</radiogroup>
										</row>
										<row if="${tipoDocumento.tipoPadre != null}">
											<label class="obligatorio" value="${tipoDocumento.tipoPadre.nombre}" />
											<hbox align="center">
												<label id="lblDocPadre" style="font-weight: bold;">
													<attribute name="onCreate">
														<![CDATA[
														    if (solicitud.getPadre() != null)
														    {
																self.setValue(solicitud.getPadre().getTitulo() + " (" + solicitud.getPadre().getCodigo() + ")");
														    }
														]]>
													</attribute>
												</label>
												<button label="${labels.buscar}" image="/images/find.png" disabled="${desactivo}">
													<attribute name="onClick">
														<![CDATA[
															Map parametros = new HashMap();
															List lista = new ArrayList();
															lista.add(solicitud.getTipoDocumento().getTipoPadre());
															
															parametros.put("tipos", lista);
															parametros.put("estado", "Vigente");
															parametros.put("seleccion", "unica");
															
															Window window = (Window)Executions.createComponents("/WEB-INF/ventanas/catDocumentos.zul", null,parametros);
															window.doModal();
															if (window.getAttribute("accion").equals("A"))
															{
																org.modelo.dto.Documento docp = window.getAttribute("objeto");
																solicitud.setPadre(docp);
																lblDocPadre.setValue(docp.getTitulo() + " (" + docp.getCodigo() + ")");
																
																org.modelo.dto.Area ar = docp.getArea();
																solicitud.setArea(ar);
																solicitud.setSubArea(docp.getSubArea());
																
																if (ar.getSubAreas().size() > 0)
																{
		
																	org.modelo.dto.RutaAprobacionSeguridad ras = servicioAdministrarConfiguracion.buscarAprobadoresPorTipoArea(solicitud.getTipoDocumento(), solicitud.getArea(), solicitud.getSubArea());
																	if (ras != null)
																	{
																		for(org.modelo.dto.RutaAprobacionAprobador raa : ras.getAprobadores())
																		{
																			org.modelo.dto.SolicitudAprobador da = new org.modelo.dto.SolicitudAprobador();
																			da.setAprobador(raa.getAprobador());
																			da.setAprobadorid(raa.getAprobadorid());
																			da.setDocumento(solicitud);
																			da.setOrden(raa.getOrden());
																			da.setDiasAprobacion(raa.getDiasAprobacion());
																			solicitud.getAprobadores().add(da);
																		}
																	}
																}
																else
																{
																	org.modelo.dto.RutaAprobacionSeguridad ras = servicioAdministrarConfiguracion.buscarAprobadoresPorTipoArea(solicitud.getTipoDocumento(), solicitud.getArea(), solicitud.getArea());
																	if (ras != null)
																	{
																		for(org.modelo.dto.RutaAprobacionAprobador raa : ras.getAprobadores())
																		{
																			org.modelo.dto.SolicitudAprobador da = new org.modelo.dto.SolicitudAprobador();
																			da.setAprobador(raa.getAprobador());
																			da.setAprobadorid(raa.getAprobadorid());
																			da.setDocumento(solicitud);
																			da.setOrden(raa.getOrden());
																			da.setDiasAprobacion(raa.getDiasAprobacion());
																			solicitud.getAprobadores().add(da);
																		}
																	}
																}
																binder.loadAll();
															}
														]]>
													</attribute>
												</button>
											</hbox>
										</row>
										<row if="${tipoDocumento.tipoPadre == null}">
											<label class="obligatorio" value="Area" />
											<hbox align="center">
												<label id="lblArea" style="font-weight: bold;"  value="@{solicitud.area.nombre}"/>
												<button label="${labels.buscar}" image="/images/find.png" disabled="${desactivo}">
													<attribute name="onClick">
														<![CDATA[
															Map parametros = new HashMap();
															parametros.put("modo","A");
															Window window = (Window)Executions.createComponents("/WEB-INF/ventanas/catAreas.zul", null,parametros);
															window.doModal();
															if (window.getAttribute("accion").equals("A"))
															{
																org.modelo.dto.Area ar = window.getAttribute("objeto");
																solicitud.setArea(ar);
																lblArea.setValue(ar.getNombre());
																lblSubArea.setValue("");
																
																if (ar.getSubAreas().size() > 0)
																{
		
																	solicitud.getAprobadores().clear();
																	solicitud.setSubArea(null);
																	rowSubArea.setVisible(true);
																}
																else
																{
																	org.modelo.dto.RutaAprobacionSeguridad ras = servicioAdministrarConfiguracion.buscarAprobadoresPorTipoArea(solicitud.getTipoDocumento(), solicitud.getArea(), solicitud.getArea());
																	if (ras != null)
																	{
																		for(org.modelo.dto.RutaAprobacionAprobador raa : ras.getAprobadores())
																		{
																			org.modelo.dto.SolicitudAprobador da = new org.modelo.dto.SolicitudAprobador();
																			da.setAprobador(raa.getAprobador());
																			da.setAprobadorid(raa.getAprobadorid());
																			da.setDocumento(solicitud);
																			da.setOrden(raa.getOrden());
																			da.setDiasAprobacion(raa.getDiasAprobacion());
																			solicitud.getAprobadores().add(da);
																		}
																	}
																	solicitud.setSubArea(ar);
																	rowSubArea.setVisible(false);
																}
																binder.loadAll();
															}
														]]>
													</attribute>
												</button>
											</hbox>									
										</row>
										<row id="rowSubArea" visible="false">
											<label class="obligatorio" value="Sub Area" />
											<hbox>
												<label id="lblSubArea" style="font-weight: bold;" value="@{solicitud.subArea.nombre}"/>
												<button label="${labels.buscar}" image="/images/find.png" disabled="${desactivo}">
													<attribute name="onClick">
														<![CDATA[
															Map parametros = new HashMap();
															parametros.put("modo","S");
															Window window = (Window)Executions.createComponents("/WEB-INF/ventanas/catAreas.zul", null,parametros);
															window.doModal();
															if (window.getAttribute("accion").equals("A"))
															{
																org.modelo.dto.Area sar = window.getAttribute("objeto");
																solicitud.setSubArea(sar);
																lblSubArea.setValue(sar.getNombre());
																
																org.modelo.dto.RutaAprobacionSeguridad ras = servicioAdministrarConfiguracion.buscarAprobadoresPorTipoArea(solicitud.getTipoDocumento(), solicitud.getArea(), solicitud.getSubArea());
																if (ras != null)
																{
																	for(org.modelo.dto.RutaAprobacionAprobador raa : ras.getAprobadores())
																	{
																		org.modelo.dto.SolicitudAprobador da = new org.modelo.dto.SolicitudAprobador();
																		da.setAprobador(raa.getAprobador());
																		da.setAprobadorid(raa.getAprobadorid());
																		da.setDocumento(solicitud);
																		da.setOrden(raa.getOrden());
																		da.setDiasAprobacion(raa.getDiasAprobacion());
																		solicitud.getAprobadores().add(da);
																	}
																}
																binder.loadAll();
															}
														]]>
													</attribute>
												</button>
											</hbox>									
										</row>
										<row>
											<label value="${labels.elaboradoPor}" />
											<label value="${solicitud.usuarioElaborador.nombres} ${solicitud.usuarioElaborador.apellidos}"/>									
										</row>
										<row>
											<label value="${labels.cargo}" />
											<label value="${solicitud.cargoElaborador}"/>									
										</row>
										<row>
											<label value="${labels.fechaDeElaboracion}" />
											<label value="${solicitud.fechaElaboracion}"/>									
										</row>
										<row spans="2">
											<label class="obligatorio" value="${labels.datosObligatorios}" />
										</row>
									</rows>
								</grid>
		                  </tabpanel>
		                  <tabpanel style="overflow:auto">
								<grid id="grdInfo" height="500px">
									<attribute name="onCreate">
										<![CDATA[
										         
										    Rows rws = self.getRows();
										    tipoDocumento.ordenarCamposDesc();
										    
											for (org.modelo.dto.Campo cmp : tipoDocumento.getCampos())
											{
												org.modelo.dto.Valor valEnc = null;
												if (modo.equals("E") || modo.equals("C"))
												{
													for (org.modelo.dto.Valor val : solicitud.getValores())
													{
														if (val.getCampo().equals(cmp))
														{
															valEnc = val;
															break;
														}
													}
												}
												
												if (cmp.getEstatus().equals("Activo"))
												{
													Row rw = new Row();
													Label lblCampo = new Label(cmp.getNombre());
													if (cmp.isObligatorio())
													{
														lblCampo.setClass("obligatorio");
													}
													lblCampo.setParent(rw);
													
													Component comp;
													if (cmp.getTipo().equals("Caja de Texto"))
													{	
														if (desactivo)
														{
															comp = new Label(valEnc.getValor());	
														}
														else
														{
															if(cmp.getTipoDato().equals("Texto"))
															{
																comp = new Textbox();
																if (valEnc != null)
																{
																	((Textbox) comp).setValue(valEnc.getValor());
																}
															}
															else if(cmp.getTipoDato().equals("Numerico Enteros"))
															{
																comp = new Intbox();
																if (valEnc != null)
																{
																	((Intbox) comp).setValue(new Integer(valEnc.getValor()));
																}
															}
															else if(cmp.getTipoDato().equals("Numerico Decimales"))
															{
																comp = new Doublebox();
																if (valEnc != null)
																{
																	((Doublebox) comp).setValue(new Double(valEnc.getValor()));
																}
															}
															
															if (cmp.getLongAncho() == null)
															{
																((org.zkoss.zul.impl.InputElement)comp).setWidth("100%");
															}
															else
															{
																((org.zkoss.zul.impl.InputElement)comp).setCols(cmp.getLongAncho());
															}
															((org.zkoss.zul.impl.InputElement)comp).setMaxlength(cmp.getCantMax());
														}
													}
													else if (cmp.getTipo().contains("Area de Texto Simple"))
													{	
														comp = new Textbox();
														
														if (cmp.getLongAncho() == null)
														{
															((Textbox)comp).setWidth("100%");
														}
														else
														{
															((Textbox)comp).setCols(cmp.getLongAncho());
														}
														
														if (cmp.getCantMax() != null)
														{
															((Textbox)comp).setMaxlength(cmp.getCantMax());
														}
														((Textbox)comp).setRows(cmp.getLongAlto());
														if (valEnc != null)
														{
															((Textbox) comp).setValue(valEnc.getValor());
														}
														((Textbox)comp).setDisabled(desactivo);
													}
													else if (cmp.getTipo().contains("Area de Texto Ampliada"))
													{	
														if (solicitud.getEstatus().equals("Nueva") || solicitud.getEstatus().equals("Rechazado"))
														{	
															comp = new org.zkforge.ckez.CKeditor();
															((org.zkforge.ckez.CKeditor)comp).setWidth("100%");
															((org.zkforge.ckez.CKeditor)comp).setHeight(cmp.getLongAlto().toString());
															((org.zkforge.ckez.CKeditor)comp).setFilebrowserImageBrowseUrl("/widget/img");
															((org.zkforge.ckez.CKeditor)comp).setFilebrowserImageUploadUrl("/widget/img");
															((org.zkforge.ckez.CKeditor)comp).setCustomConfigurationsPath("/js/config.js");
															if (valEnc != null)
															{
																((org.zkforge.ckez.CKeditor) comp).setValue(valEnc.getValor());
															}
														} 
														else if (solicitud.getEstatus().equals("En Revision") || solicitud.getEstatus().equals("Aprobado") || solicitud.getEstatus().equals("Vigente"))
														{
															comp = new Html();
															((Html)comp).setContent(valEnc.getValor());
														}
														else
														{
															comp = new Html();
															((Html)comp).setContent(valEnc.getValor());
														}
													}
													else if (cmp.getTipo().contains("Opciones Simples"))
													{	
														comp = new Radiogroup();
														for(org.modelo.dto.Opcion ops : cmp.getOpciones())
														{
															Radio rd = new Radio();
															if (ops.getEstatus().equals("Activo"))
															{
																rd.setLabel(ops.getNombre());	
																rd.setValue(ops.getNombre());
																
																if (valEnc != null)
																{
																	if (ops.getNombre().equals(valEnc.getValor()))
																	{
																		rd.setChecked(true);
																	}
																}
															}
															rd.setId(cmp.getCodigo().toString() + "-" + ops.getCodigo().toString());
															rd.setParent(comp);
														}
														
													}
													else if (cmp.getTipo().contains("Opciones Multiples"))
													{	
														comp = new Hbox();
														for(org.modelo.dto.Opcion ops : cmp.getOpciones())
														{
															Checkbox chk = new Checkbox();
															if (ops.getEstatus().equals("Activo"))
															{
																chk.setLabel(ops.getNombre());	
																chk.setValue(ops.getNombre());
																
																if (valEnc != null)
																{
																	for (org.modelo.dto.ValorOpcion valo : valEnc.getValoresOpciones())
																	{
																		if (valo.getValorResul().equals(ops.getNombre()))
																		{
																			chk.setChecked(true);
																		}
																	}
																}
															}
															chk.setId(cmp.getCodigo().toString() + "-" + ops.getCodigo().toString());
															chk.setParent(comp);
														}
														
													}
													else if (cmp.getTipo().contains("Referencia"))
													{
														comp = new Hbox();
														Button btnRef = null;
														Label lblRef = null;
														if (cmp.getCantidadReferencia().equals(new Integer(1)))
														{
															lblRef = new Label();
															lblRef.setParent(comp);
															btnRef = new Button("Buscar");
															btnRef.setParent(comp);
														}
															
														if (cmp.getTipoReferencia().equals("A Documentos"))
														{ 
															if (btnRef != null)
															{
																rw.setValue(cmp);
																btnRef.addEventListener("onClick",
																	new org.zkoss.zk.ui.event.EventListener() {
																		public void onEvent(Event event) {
																			Map parametros = new HashMap();
																			
